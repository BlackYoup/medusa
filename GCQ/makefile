PLATFORM=unix

ifneq ("$(windir)","")
PLATFORM=windows
endif

ifneq ("$(WinDir)","")
PLATFORM=windows
endif

ifneq ("$(WINDIR)","")
PLATFORM=windows
endif

UNAME=$(shell uname -o)

ifeq ("$(UNAME)","Cygwin")
PLATFORM=cygwin
endif

ifeq ("$(BUILD_CONFIGURATION)","")
	BUILD_CONFIGURATION=ReleaseLinux
endif

ifeq ("$(BUILD_CONFIGURATION)","DebugLinux")
CC= gcc
CXX= g++
AR= ar
CFLAGS= -g -I ../
CXXFLAGS= -g -I ../ -sdt=c++0x
ARFLAGS= cru
LDFLAGS= -shared
RESULT=libGCQD.so
INTERMEDIATE_DIR=DebugLinux/obj
DESTINATION_DIR=../Bin
MKDIR= mkdir -p
RM= rm -f

endif

ifeq ("$(BUILD_CONFIGURATION)","ReleaseLinux")
CC= gcc
CXX= g++
AR= ar
CFLAGS= -g -O2 -I ../
CXXFLAGS= -g -O2 -I ../ -std=c++0x
ARFLAGS= cru
LDFLAGS= -shared
RESULT=libGCQ.so
INTERMEDIATE_DIR=ReleaseLinux/obj
DESTINATION_DIR=../Bin
MKDIR= mkdir -p
RM= rm -f

endif

SOURCES= MasterClient.cpp MetaClient.cpp MetaClientCommand.cpp MetaClientMID.cpp ProcessClient.cpp

SOURCEPATHS= MasterClient.cpp MetaClient.cpp MetaClientCommand.cpp MetaClientMID.cpp ProcessClient.cpp

HEADERS= Binary.h GCQDll.h MasterClient.h MetaClient.h PBM.h ProcessClient.h Resource.h

VPATH= $(INTERMEDIATE_DIR)

OBJECTNAMES=$(patsubst %.c,%.o,$(patsubst %.cpp,%.o,$(SOURCES)))
OBJECTPATHS=$(addprefix $(INTERMEDIATE_DIR)/,$(OBJECTNAMES))

$(DESTINATION_DIR)/$(RESULT): $(OBJECTNAMES)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR)
endif
	$(CXX) -shared $(CXXFLAGS) $(SYSROOTFLAGS) $(OBJECTPATHS) -o $@ $(LDFLAGS)

$(INTERMEDIATE_DIR)/GCQ.d: $(SOURCES) $(HEADERS)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR)
endif
ifeq ("$(wildcard $(INTERMEDIATE_DIR) )","")
	-$(MKDIR) $(INTERMEDIATE_DIR)
endif
	$(CXX) $(CXXFLAGS) -MM $(SOURCEPATHS) > $@

-include $(INTERMEDIATE_DIR)/GCQ.d

clean:
	$(RM) $(DESTINATION_DIR)/$(RESULT)
	$(RM) $(INTERMEDIATE_DIR)/GCQ.d
	$(RM) $(OBJECTPATHS)

%.o: %.cpp
	$(CXX) -fPIC $(CXXFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<

%.o: %.c
	$(CC) -fPIC $(CFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<
